{
    "dataform-version": "1.0.0",
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [{
            "spanLabel": true,
            "type": "label",
            "valueClass": "nowidth",
            "value": "${dataform.title}",
            "cssClass": "title"
        }, {
            "type": "textbox",
            "label": "${dataform.link}",
            "field": "src",
            "required": true,
            "size": {
                "w": 400
            }
        }]
}